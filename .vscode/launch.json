    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug-init-help",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": ["init", "--help"]
        },
        {
            "name": "debug-init-project-basic",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": ["init", "project", "--name", "generated-basic-project"]
        },
        {
            "name": "debug-init-project-terraform",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": ["init", "project", "--name", "generated-terraform-project", "--type", "terraform"],
        },
        {
            "name": "debug-init-project-terraform-with-only-customized-structure",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": ["init", "project", "--name", "generated-terraform-project-only-customized-structure", "--type", "terraform", "-o"],
        }
    ]
}